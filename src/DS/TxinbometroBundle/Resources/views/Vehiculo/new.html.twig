{# src/DS/TxinbometroBundle/Resources/views/Vehiculo/new.html.twig #}
{% extends "TxinbometroBundle::layout.html.twig" %}

{% block stylesheets %}
{{ parent() }}
<link href="{{ asset('/css/ui-lightness/jquery-ui-1.8.16.custom.css') }}" rel="stylesheet" type="text/css" /></link>
{% endblock %}

{% block javascript %}
{{ parent() }}
    <script src="{{ asset('/js/jquery-1.6.2.min.js') }}" type="text/javascript"></script>
    <script src="{{ asset('/js/jquery-ui-1.8.16.custom.min.js') }}" type="text/javascript"></script>
{% endblock %}

{% block menu %} {% render "TxinbometroBundle:Default:menu" with { 'opcion': 'Vehiculo', 'subopcion': 'none' } %} {% endblock %}

{% block body %}
<h1>Vehiculo creation</h1>

<form action="{{ path('txinbometro_vehiculo_create') }}" method="post" {{ form_enctype(form) }}>
    {{ form_widget(form) }}
    <p>
        <button type="submit">Create</button>
    </p>
</form>

<ul class="record_actions">
    <li>
        <a href="{{ path('txinbometro_vehiculo') }}">
            Back to the list
        </a>
    </li>
</ul>

