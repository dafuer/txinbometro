<h1>Moto</h1>

<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>                <td>{{ entity.id }}</td>        </tr>
        <tr>
            <th>Marca</th>                <td>{{ entity.marca }}</td>        </tr>
        <tr>
            <th>Modelo</th>                <td>{{ entity.modelo }}</td>        </tr>
        <tr>
            <th>Compra</th>                <td>{{ entity.compra|date('Y-m-d H:i:s') }}</td>        </tr>
        <tr>
            <th>Coste</th>                <td>{{ entity.coste }}</td>        </tr>
        <tr>
            <th>Km_iniciales</th>                <td>{{ entity.kminiciales }}</td>        </tr>
        <tr>
            <th>Capacidad</th>                <td>{{ entity.capacidad }}</td>        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('moto') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('moto_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('moto_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
